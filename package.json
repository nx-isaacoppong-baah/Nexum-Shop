    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.9.3",
    "@remix-run/express": "^1.5.1",
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@storyblok/js": "^1.6.1",
    "@storyblok/react": "^1.0.8",
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "framer-motion": "^6.3.11",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-country-flag": "^3.0.2",
