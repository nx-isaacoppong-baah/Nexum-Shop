    "eslint": "^8.15.0",
    "mv": "^2.1.1",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
